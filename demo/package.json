{
  "name": "onboarding-web-app",
  "version": "0.1.0",
  "description": "Onboarding Web App",
  "main": "src/index.js",
  "repository": "",
  "scripts": {
    "build": "rm -rf dist; mkdir dist; webpack --display-error-details --config src/dev/webpack/config.production.js",
    "build:dev": "rm -rf dist; mkdir dist; webpack --display-error-details --config src/dev/webpack/config.dev.js",
    "dev": "npm run build:dev & npm run server:dev",
    "lint": "eslint src/app src/server src/shared --ext .js,.jsx; stylelint src/scss/**/*.scss || true",
    "server:dev": "nodemon src/server --config ./src/dev/nodemon.json",
    "start": "NODE_ENV=production; babel-node src/server",
    "test": "NODE_ENV=test mocha --opts src/dev/mocha.opts || true"
  },
  "author": "Thomas Andreo",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "6.25.0",
